{
  "name": "starter",
  "version": "1.0.0",
  "description": "Node Application Boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -e js,graphql --exec babel-node src/index.js",
    "prebuild": "rimraf dist/*",
    "build": "babel -s -d ./dist ./src --copy-files",
    "prestart": "npm run build",
    "start": "node dist/index.js"
  },
  "author": "Robert Barnwell <robert@robertismy.name>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@sentry/node": "^5.6.1",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "mysql2": "^1.6.5",
    "sequelize": "^5.13.0",
    "winston": "^3.2.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {}
  }
}
